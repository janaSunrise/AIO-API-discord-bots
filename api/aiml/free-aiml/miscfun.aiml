<topic name="BOTJOKE">
<category>
<pattern>_</pattern>
<template>
<think>
<set name="answr"><star/></set>
<set name="message">wrong answer</set>
<condition name="answr">

<li value="WHY GUESS"><set name="message">why guess</set></li>
<li value="* WHY GUESS"><set name="message">why guess</set></li>
<li value="WHY * GUESS"><set name="message">why guess</set></li>
